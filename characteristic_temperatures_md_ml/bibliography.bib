@article{Puosi2018,
abstract = {As approaching the glass transition, particle motion in liquids becomes highly heterogeneous and regions with virtually no mobility coexist with liquid-like domains. This complex dynamics is believed to be responsible for different phenomena including non-exponential relaxation and the breakdown of Stokes-Einstein relation. Understanding the relationships between dynamical heterogeneities and local structure in metallic liquids and glasses is a major scientific challenge. Here we use classical molecular dynamics simulations to study the atomic dynamics and microscopic structure of {\$}\backslashmbox{\{}Cu{\}}{\_}{\{}50{\}}\backslashmbox{\{}Zr{\}}{\_}{\{}50{\}}{\$} alloy in the supercooling regime. Dynamical heterogeneities are identified via an isoconfigurational analysis. As deeper supercooling is achieved a transition from isolated to clustering low mobility atoms is reported. These slow clusters, whose size grow upon cooling, are also associated to concentration fluctuations, characterized by a Zr-enriched phase, with a composition {\$}\backslashmbox{\{}CuZr{\}}{\_}2{\$}. In addition, a structural analysis of slow clusters based on Voronoi tessellation evidences an increase with respect of the bulk system of the fraction of Cu atoms having a local icosahedral order. These results are in agreement with the consolidated scenario of the relevant role played by icosahedral order in the dynamic slowing-down in supercooled metal alloys.},
author = {Puosi, F. and Jakse, N. and Pasturel, A.},
doi = {10.1088/1361-648X/aab110},
file = {:C$\backslash$:/Users/Nerve/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Puosi, Jakse, Pasturel - 2018 - Dynamical, structural and chemical heterogeneities in a binary metallic glass-forming liquid.pdf:pdf},
issn = {1361648X},
journal = {Journal of Physics Condensed Matter},
keywords = {Metallic liquids and glasses,heterogeneities,molecular dynamics simulations,structure and dynamics},
mendeley-groups = {Viscosity},
month = {3},
number = {14},
publisher = {Institute of Physics Publishing},
title = {{Dynamical, structural and chemical heterogeneities in a binary metallic glass-forming liquid}},
volume = {30},
year = {2018}
}

@article{Plimpton1995,
abstract = {Three parallel algorithms for classical molecular dynamics are presented. The first assigns each processor a fixed subset of atoms; the second assigns each a fixed subset of inter-atomic forces to compute; the third assigns each a fixed spatial region. The algorithms are suitable for molecular dynamics models which can be difficult to parallelize efficiently - those with short-range forces where the neighbors of each atom change rapidly. They can be implemented on any distributed-memory parallel machine which allows for message-passing of data between independently executing processors. The algorithms are tested on a standard Lennard-Jones benchmark problem for system sizes ranging from 500 to 100,000,000 atoms on several parallel supercomputers - the nCUBE 2, Intel iPSC/860 and Paragon, and Cray T3D. Comparing the results to the fastest reported vectorized Cray Y-MP and C90 algorithm shows that the current generation of parallel machines is competitive with conventional vector supercomputers even for small problems. For large problems, the spatial algorithm achieves parallel efficiencies of 90{\%} and a 1840-node Intel Paragon performs up to 165 faster than a single Cray C9O processor. Trade-offs between the three algorithms and guidelines for adapting them to more complex molecular dynamics simulations are also discussed. {\textcopyright} 1995 Academic Press, Inc.},
author = {Plimpton, Steve},
doi = {10.1006/jcph.1995.1039},
file = {:C$\backslash$:/Users/Nerve/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Plimpton - 1995 - Fast parallel algorithms for short-range molecular dynamics.pdf:pdf},
issn = {00219991},
journal = {Journal of Computational Physics},
mendeley-groups = {Glassy Metals Descriptors,Molecular Dynamics},
month = {3},
number = {1},
pages = {1--19},
title = {{Fast parallel algorithms for short-range molecular dynamics}},
volume = {117},
year = {1995}
}

