file = {:C$\backslash$:/Users/Nerve/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Blodgett et al. - 2015 - Proposal for universality in the viscosity of metallic liquids.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
mendeley-groups = {Glassy Metals,Characteristic Temperatures MD ML},
pages = {1--8},
pmid = {26350788},
publisher = {Nature Publishing Group},
title = {{Proposal for universality in the viscosity of metallic liquids}},
url = {http://dx.doi.org/10.1038/srep13837},
volume = {5},
year = {2015}
}

