file = {:C$\backslash$:/Users/Nerve/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mendelev et al. - 2009 - Development of suitable interatomic potentials for simulation of liquid and amorphous Cu-Zr alloys.pdf:pdf},
issn = {14786435},
journal = {Philosophical Magazine},
keywords = {Diffraction methods,Interatomic potential,Molecular dynamic simulations},
mendeley-groups = {Glass Transition Temperature,Characteristic Temperatures MD ML},
number = {11},
pages = {967--987},
title = {{Development of suitable interatomic potentials for simulation of liquid and amorphous Cu-Zr alloys}},
volume = {89},
year = {2009}
}

